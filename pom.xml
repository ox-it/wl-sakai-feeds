<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>master</artifactId>
		<groupId>org.sakaiproject</groupId>
		<version>2.5.0</version>              <!-- Version number for tagged releases (eg, 2.5.0) -->
		<!-- <version>M2</version> -->        <!-- M2 for maintenance branches (eg, 2.5.x) -->
		<!-- <version>SNAPSHOT</version> -->  <!-- SNAPSHOT for trunk -->
	</parent>

	<properties>
		<!-- This tool VERSION -->
		<feeds.version>SNAPSHOT</feeds.version>
		<!-- Dependencies VERSION -->
		<entitybroker.version>[1.2.0,1.4.0]</entitybroker.version>
		<feeds.wicket.version>1.3.3</feeds.wicket.version>
		<feeds.wicketstuff-dojo.version>1.3.0-beta</feeds.wicketstuff-dojo.version>
		<feeds.slf4j.version>1.4.3</feeds.slf4j.version>
	</properties>

	<organization>
		<name>Universidade Fernando Pessoa</name>
		<url>http://www.ufp.pt</url>
	</organization>
	<inceptionYear>2008</inceptionYear>

	<name>Sakai Feeds</name>
	<groupId>org.sakaiproject</groupId>
	<artifactId>sakai-feeds</artifactId>
	<version>${feeds.version}</version>

	<packaging>pom</packaging>
	<modules>
		<module>api</module>
		<module>impl</module>
		<module>pack</module>
		<module>tool</module>
	</modules>


	<!-- Maven repositories -->
	<repositories>
		<repository>
			<id>wicket-stuff-repository</id>
			<name>Wicket-Stuff Repository</name>
			<url>http://www.wicketstuff.org/maven/repository/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>


	<!-- Handles the management of all related dependencies -->
	<dependencyManagement>
		<dependencies>
			<!-- Internal: Sakai Feeds -->
			<dependency>
				<groupId>org.sakaiproject</groupId>
				<artifactId>sakai-feeds-api</artifactId>
				<version>${feeds.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.sakaiproject</groupId>
				<artifactId>sakai-feeds-impl</artifactId>
				<version>${feeds.version}</version>
			</dependency>

			<!-- Internal: Sakai -->
			<dependency>
				<groupId>org.sakaiproject.entitybroker</groupId>
				<artifactId>entitybroker-api</artifactId>
				<version>${entitybroker.version}</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>org.sakaiproject.entitybroker</groupId>
				<artifactId>entitybroker-utils</artifactId>
				<version>${entitybroker.version}</version>
			</dependency>
			<dependency>
				<groupId>org.sakaiproject</groupId>
				<artifactId>sakai-util</artifactId>
				<version>${sakai.version}</version>
			</dependency>

			<!-- External -->
			<dependency>
				<groupId>commons-logging</groupId>
				<artifactId>commons-logging</artifactId>
				<version>1.1</version>
			</dependency>
			<dependency>
				<groupId>commons-codec</groupId>
				<artifactId>commons-codec</artifactId>
				<version>1.3</version>
			</dependency>
			<dependency>
				<groupId>commons-httpclient</groupId>
				<artifactId>commons-httpclient</artifactId>
				<version>3.1</version>
			</dependency>
			<dependency>
				<groupId>commons-httpclient</groupId>
				<artifactId>commons-httpclient-contrib</artifactId>
				<version>3.0-beta1</version>
			</dependency>
			<dependency>
				<groupId>rome</groupId>
				<artifactId>rome</artifactId>
				<version>0.9</version>
			</dependency>
			<dependency>
				<groupId>rome</groupId>
				<artifactId>rome-fetcher</artifactId>
				<version>0.7</version>
			</dependency>

			<!-- External: Apache Wicket dependencies -->
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket-extensions</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket-ioc</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket-spring</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket-spring-annot</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.wicket</groupId>
				<artifactId>wicket-datetime</artifactId>
				<version>${feeds.wicket.version}</version>
			</dependency>
			<dependency>
				<groupId>org.wicketstuff</groupId>
				<artifactId>wicketstuff-dojo</artifactId>
				<version>${feeds.wicketstuff-dojo.version}</version>
			</dependency>
			<!-- <dependency>
				<groupId>org.wicketstuff.jquery</groupId>
				<artifactId>wicketstuff-jquery</artifactId>
				<version>0.1-SNAPSHOT</version>
				</dependency> -->
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-api</artifactId>
				<version>${feeds.slf4j.version}</version>
			</dependency>
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-log4j12</artifactId>
				<version>${feeds.slf4j.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
